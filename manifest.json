{
    "manifest_version": 3,
    "name": "Tempomail Generator",
    "version": "1.0",
    "description": "Generate temporary emails and check inbox within a Chrome extension.",
    "permissions": ["storage", "activeTab", "scripting"],
    "host_permissions": ["http://localhost:5000/*"],
    "action": {
      "default_popup": "index.html"
      
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  